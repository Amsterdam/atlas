  },
  "husky": {
    "hooks": {
      "pre-push": "npm run test-jest -- --bail && npm run test-karma",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "prettier --write",
      "eslint",
      "git add"
    ],
    "*.scss": [
      "sass-lint"
    ]
  }
