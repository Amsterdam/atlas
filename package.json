  "license": "MPL-2.0",
  "pre-commit": "lint",
  "scripts": {
    "build": "npm run build-prod",
    "build-acc": "webpack --config webpack.acc.js",
    "build-dev": "webpack --config webpack.dev.js",
    "build-prod": "webpack --config webpack.prod.js",
    "build-test": "webpack --config webpack.acc.js",
    "clean": "npm run clean-cache && npm run clean-dirs",
    "clean-cache": "npm cache verify",
    "clean-dirs": "rimraf ./node_modules ./coverage ./coverage-legacy ./dist ./build",
    "lint": "npm run lint:js && npm run lint:style",
    "lint:js": "eslint --format=node_modules/eslint-formatter-pretty src modules test/e2e/cypress --ext js,jsx",
    "lint:style": "sass-lint -c src/.sass-lint.yml 'src/**/*.scss' --verbose --no-exit --ignore 'src/shared/styles/config/mixins/_sprites.scss' && sass-lint -c modules/.sass-lint.yml 'modules/**/*.scss' --verbose --no-exit",
    "preinstall": "npm cache verify",
    "start": "webpack-dev-server --config webpack.dev.js",
    "start-prod": "webpack-dev-server --config webpack.prod-api-dev.js",
    "test": "npm run lint && cross-env karma start && jest --config=./test/jest.unit.config.js",
    "test-eslint": "eslint ./modules/ ./src/ ./test/cypress/",
    "test-integration": "cross-env NODE_ENV=development jest --config=./test/jest.integration.config.js",
    "test-integration-watch": "cross-env NODE_ENV=development jest --config=./test/jest.integration.config.js --watch",
    "test-jest": "cross-env NODE_ENV=development jest --config=./test/jest.unit.config.js --runInBand",
    "test-jest-watch": "cross-env NODE_ENV=development jest --config=./test/jest.unit.config.js --watch --onlyChanged --coverage --runInBand",
    "test-karma": "karma start",
    "test-karma-watch": "karma start --no-single-run"
  },
  "dependencies": {
    "@datapunt/asc-assets": "^0.6.3-alpha.0",
