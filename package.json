  "scripts": {
    "build": "npm run build-prod",
    "build-acc": "grunt && webpack --config webpack.acc.js",
    "build-dev": "grunt && webpack --config webpack.dev.js",
    "build-prod": "grunt && npm run lint && npm test && webpack --config webpack.prod.js",
    "build-test": "grunt && webpack --config webpack.acc.js",
    "clean-cache": "npm cache verify",
    "clean-dirs": "rimraf ./node_modules ./coverage ./coverage-legacy ./dist ./build",
    "clean": "npm run clean-cache && npm run clean-dirs",
    "lint": "eslint --config ./.eslintrc.js ./modules/ && eslint --ext .js --ext .jsx -c ./src/.eslintrc.js --ignore-path ./src/.eslintignore ./src/",
    "postinstall": "./node_modules/protractor/node_modules/webdriver-manager/bin/webdriver-manager update",
    "preinstall": "npm cache verify",
    "start": "grunt && webpack-dev-server --config webpack.dev.js",
    "test": "karma start && NODE_ENV=development jest",
    "test-aria": "node ./test/pa11y.js",
    "test-e2e": "babel-node ./test/e2e.js",
    "test-jest": "NODE_ENV=development jest",
    "test-jest-update": "NODE_ENV=development jest --updateSnapshot",
    "test-karma": "karma start",
    "test-watch": "karma start --no-single-run & jest --watch",
    "watch": "grunt && webpack --watch --config webpack.dev.js"
  },
