  "scripts": {
    "build": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config ./webpack.prod.ts",
    "build-storybook": "build-storybook -c .storybook -o .sb",
    "clean": "npm run clean:dirs",
    "clean:dirs": "rimraf ./node_modules ./coverage ./coverage-legacy ./dist ./build",
    "deploy-storybook": "storybook-to-ghpages",
    "generate:sitemap": "node ./sitemap-generator/index.js",
    "lint": "npm run lint:js",
    "lint:js": "eslint --format=node_modules/eslint-formatter-pretty src modules test/e2e/cypress --ext js,jsx,ts,tsx",
    "start": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --config webpack.dev.ts",
    "start:prod": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack-dev-server --config webpack.prod.ts",
    "storybook": "start-storybook -p 6007",
    "test": "concurrently \"npm run test:karma\" \"jest\"",
    "test:jest": "jest --coverage",
    "test:jest:watch": "npm run test:jest -- --watch --onlyChanged",
    "test:karma": "cross-env TS_NODE_TRANSPILE_ONLY=true karma start",
    "test:karma:watch": "npm run test:karma -- --no-single-run",
    "prettier": "prettier --write \"./**/*.{js,jsx,ts,tsx,scss,css}\"",
    "analyze:modern": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.analyze.ts --env.config=modern",
    "analyze:legacy": "cross-env TS_NODE_PROJECT=tsconfig.webpack.json webpack --config webpack.analyze.ts --env.config=legacy"
  },
