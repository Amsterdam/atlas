{
  "extends": "airbnb",
  "env": {
    "browser": true,
    "jest": true,
    "node": true
  },
  "root": true,
  "rules": {
    "arrow-parens": ["error", "always"],
    "comma-dangle": ["error", "never"],
    "no-nested-ternary": "off",
    "no-underscore-dangle": ["error", {
      "allow": [
        "__BROWSER__",
        "__REDUX_DEVTOOLS_EXTENSION_COMPOSE__",
        "_display",
        "_embedded",
        "_links"
      ]
    }]
  }
}
