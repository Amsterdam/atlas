{
  "compilerOptions": {
    "strict": true,
    "jsx": "react",
    "resolveJsonModule": true,
    "allowJs": true,
    "esModuleInterop": true,
    "downlevelIteration": true,
    "suppressImplicitAnyIndexErrors": true,
    "lib": ["ES2020", "DOM", "DOM.Iterable"],
    "typeRoots": [
      "src/typings",
      "node_modules/@types"
    ]
  }
}
